{
  "name": "nanocomponent-adapters",
  "description": "Adapters to make nanocomponent run natively inside frameworks",
  "repository": "choojs/nanocomponent-adapters",
  "version": "3.0.0",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "start": "NODE_ENV=production bankai examples/react.js --open",
    "test": "standard && npm run deps"
  },
  "dependencies": {
    "nanocomponent-adapters-angular": "^1.0.1",
    "nanocomponent-adapters-angularjs": "^1.0.0"
  },
  "devDependencies": {
    "@angular/core": "^2.0.0",
    "bankai": "^8.1.1",
    "dependency-check": "^2.7.0",
    "nanocomponent": "^6.0.0-1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "standard": "^10.0.3"
  },
  "keywords": [
    "nanocomponent",
    "custom-element",
    "webcomponent",
    "custom-elements"
  ]
}
